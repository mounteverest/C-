					
public class Program
{
	
	class box
	{
		public double length;
		public double height;
		public double width;
	}
	class boxtester
	{
		
		
	
	 static void Main(String[] args)
	{
		Console.WriteLine("Hello World");
		//Create an instance
		box box1 = new box();
		box box2 = new box();
		double volume = 0;
		
		//get Specification for box1
		box1.length = 12.2;
		box1.height = 20.3;
		box1.width = 10.8;
		
		//get details for box2
		box2.length = 22.3;
		box2.height = 14.7;
		box2.width = 9.8;
		
		//Volume of Box1
		volume = box1.length * box1.height * box1.width;
		Console.WriteLine("The Volume of Box1 is :{0}",volume);
		
		//Volume of Box2
		volume = box2.height * box2.width * box2.length;
		Console.WriteLine("The Volume of Box2 is : {0}", volume);
	}	
		
	}	
}

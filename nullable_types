/*

The nullable tyeps are used to declare the normal value range and the normal value for the varibales.
and its declared as int? x = null;
??coalescing operator
x?? 5
it returns the value of the second operand if the first operand is null,
otherwise it returns the value of the forst operand
*/
	
using System;
					
public class Program
{
	public static void Main()
	{
	int? x = null;
		Console.WriteLine("the value of x is {0}",x?? 3);
		string s = null;
		Console.WriteLine("The value of string is {0}",s??"sdkfsf");
		x = 5;
		Console.WriteLine("The value of {0} is :{1}",x.HasValue,x?? 10);
	}
}

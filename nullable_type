using System;
					
public class Program
{
	public static void Main()
	{
		{
        int? num = null;

        // Is the HasValue property true?
        if (num.HasValue)
        {
            Console.WriteLine("num = " + num.Value);
        }
        else
        {
            Console.WriteLine("num = Null");
        }

	
        }
	
 	 }		
}	
